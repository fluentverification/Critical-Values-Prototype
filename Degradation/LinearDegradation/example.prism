ctmc

const double kd = 0.0075 ;

const int MAX = 100;

module TetR_def

	TetR : [0..MAX] init MAX;

	[] TetR=20 -> kd * TetR/20 :(TetR'=TetR-20);
	[] TetR=40 -> kd * TetR/20 :(TetR'=TetR-20);
	[] TetR=60 -> kd * TetR/20 :(TetR'=TetR-20);
	[] TetR=80 -> kd * TetR/20 :(TetR'=TetR-20);
	[] TetR=100 -> kd * TetR/20 :(TetR'=TetR-20);

endmodule
